@{
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<MyProWeb.Models.Brand>

<div class="uk-accordion-content">

    <ul class="uk-nav uk-nav-default">
        @foreach (var brandName in Model)
        {
            <li>
                    <a asp-controller="Filter"
                       asp-action="SPTheoBrand"
                       asp-route-loaiBrand="@brandName.Idbrand">
                        @brandName.NameBrand
                        <span class="uk-text-meta uk-text-xsmall"></span>
                    </a>
                
            </li>
        }

    </ul>


</div>
